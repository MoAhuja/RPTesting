{
  "name": "@azure-tools/typespec-providerhub",
  "version": "0.38.1",
  "author": "Microsoft Corporation",
  "description": "Azure ProviderHub TypeSpec Library",
  "homepage": "https://azure.github.io/typespec-azure",
  "readme": "https://github.com/Azure/typespec-azure/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-azure.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/typespec-azure/issues"
  },
  "keywords": [
    "typespec"
  ],
  "main": "dist/src/index.js",
  "tspMain": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "peerDependencies": {
    "@typespec/compiler": "~0.52.0",
    "@azure-tools/typespec-autorest": "~0.38.0",
    "@typespec/openapi": "~0.52.0",
    "@typespec/http": "~0.52.0",
    "@typespec/rest": "~0.52.0",
    "@azure-tools/typespec-azure-core": "~0.38.0",
    "@azure-tools/typespec-azure-resource-manager": "~0.38.0",
    "@typespec/versioning": "~0.52.0"
  },
  "devDependencies": {
    "@typespec/compiler": "~0.52.0",
    "@typespec/openapi": "~0.52.0",
    "@typespec/versioning": "~0.52.0",
    "@azure-tools/typespec-azure-core": "~0.38.0",
    "@azure-tools/typespec-autorest": "~0.38.0",
    "@typespec/http": "~0.52.0",
    "@typespec/rest": "~0.52.0",
    "@azure-tools/typespec-azure-resource-manager": "~0.38.0",
    "@types/mocha": "~10.0.1",
    "@types/node": "~18.11.9",
    "@typespec/eslint-config-typespec": "~0.52.0",
    "@typespec/library-linter": "~0.52.0",
    "@typespec/eslint-plugin": "~0.52.0",
    "eslint": "^8.55.0",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.1.0",
    "c8": "~8.0.1",
    "rimraf": "~5.0.1",
    "typescript": "~5.3.3"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p . && npm run lint-typespec-library",
    "watch": "tsc -p . --watch",
    "lint-typespec-library": "tsp compile . --warn-as-error --import @typespec/library-linter --no-emit",
    "test": "vitest run",
    "test:watch": "vitest -w",
    "test-official": "vitest run --coverage --reporter=junit --reporter=default --no-file-parallelism",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts"
  }
}